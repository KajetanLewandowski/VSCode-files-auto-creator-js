{
	"name": "filesautocreatorjs",
	"displayName": "FilesAutoCreatorJS",
	"publisher": "LegwanXDL",
	"description": "",
	"repository": {
		"type": "git",
		"url": "https://github.com/LegwanXDL/VSCode-files-auto-creator-js-"
	},
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:componentautocreatorjs.createComponent",
		"onCommand:componentautocreatorjs.createIndex"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "componentautocreatorjs.createComponent",
				"title": "Create Component"
			},
			{
				"command": "componentautocreatorjs.createIndex",
				"title": "Create Index"
			}
		],
		"keybindings": [
			{
				"command": "componentautocreatorjs.createComponent",
				"key": "ctrl+e",
				"when": "explorerResourceIsFolder"
			},
			{
				"command": "componentautocreatorjs.createIndex",
				"key": "ctrl+t",
				"when": "filesExplorerFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}